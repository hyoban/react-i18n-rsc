{
  "name": "react-i18n-rsc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "imports": {
    "#i18n/useTranslation": {
      "react-server": "./src/i18n/useTranslation.server.ts",
      "default": "./src/i18n/useTranslation.client.ts"
    },
    "#i18n/settings": "./src/i18n/settings.ts",
    "#i18n/server": "./src/i18n/server.ts"
  },
  "scripts": {
    "build": "waku build",
    "dev": "waku dev",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "simple-git-hooks",
    "start": "waku start"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "cookie-es": "^2.0.0",
    "hono": "^4.11.3",
    "i18next": "^25.7.3",
    "react": "~19.2.3",
    "react-dom": "~19.2.3",
    "react-i18next": "^16.5.1",
    "react-server-dom-webpack": "~19.2.3",
    "waku": "1.0.0-alpha.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-hyoban": "^5.0.2",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3"
  }
}
